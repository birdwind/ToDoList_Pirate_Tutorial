{
  "name": "to_do_list-pirate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "yarn prettier && vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/images/icon_logo.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prettier": "yarn prettier:ts && yarn prettier:vue",
    "prettier:dev": "yarn prettier:dev-ts && yarn prettier:dev-vue",
    "prettier:dev-ts": "git diff --name-only | grep -P '\\.ts$' | xargs prettier --write ",
    "prettier:dev-vue": "git diff --name-only | grep -P '\\.vue$' | xargs prettier --write --parser vue",
    "prettier:ts": "prettier --write \"src/**/**.ts\"",
    "prettier:vue": "prettier --write \"src/**/**.vue\" --parser vue"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "electron-store": "^8.0.1",
    "element-ui": "^2.15.6",
    "linq": "3.2.1",
    "loglevel": "^1.7.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "uuid-generator-ts": "^1.1.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-click-outside": "^1.1.0",
    "vue-contextmenujs": "^1.3.13",
    "vue-property-decorator": "^9.1.2",
    "vue-right-click-menu": "^1.1.1",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-vuetify": "~2.4.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  }
}
